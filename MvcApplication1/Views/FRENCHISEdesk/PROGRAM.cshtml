                    $(".multiply_amount").val(0);
                    $('#triple_temp_total').val(0);
                    $('#jodi_temp_total').val(0);
                    event.preventDefault();
                }
                if (115 == event.keyCode) {
                    $("#Cansel_Last_Ticket").click();
                    event.preventDefault();
                }

                if (117 == event.keyCode) {
                    
                    //for 2 time f6 click add value hidden true.
                    var f_Six = $('.firstF6').val();

                    if(f_Six == 'false'){
                        // $('.firstF6').val(true);
                        $(".saveData").click();
                    }else{

                        printReceipt();
                    }
                    event.preventDefault();
                }
                if (112 == event.keyCode) {
                    location.href = "BALTRANSFERCLIENT";
                    event.preventDefault();
                }
                if (118 == event.keyCode) {
                    location.href = "/FRENCHISEdesk/demo2";
                    event.preventDefault();
                }
                if (120 == event.keyCode) {
                    location.href = "/FRENCHISEdesk/Luckydraw";
                    event.preventDefault();
                }
            }else{
                //f1 key to add bet in jodi ALL input box.
                var modalId = $('.modal.fade.in').attr('id');

                if (112 == event.keyCode) {
                    if(modalId.includes('jodiModal') == true){
                        $(".tempjAll").focus();
                        return false;
                    }
                    if(modalId.includes('tripleModal') == true){
                        $(".temptAll").focus();
                        return false;
                    }
                }
                if (113 == event.keyCode) {
                    if(modalId.includes('tripleModal') == true){
                        $(".temptSP").focus();
                        return false;
                    }
                }
                if (114 == event.keyCode) {
                    if(modalId.includes('tripleModal') == true){
                        $(".temptDP").focus();
                        return false;
                    }
                }
                if (115 == event.keyCode) {
                    if(modalId.includes('tripleModal') == true){
                        $(".temptTP").focus();
                        return false;
                    }
                }
                if (8 == event.keyCode) {
                    var focused = document.activeElement;
                    if (focused.tagName == 'INPUT') {
                        return true;
                    } else {
                        return false;
                    }
                }
                if (38 == event.keyCode) {
                    var row = $(this).closest('li').next();
                    row.find('input').focus();
                }
                if (39 == event.keyCode) {
                    $(this).next().focus();
                }
                if (37 == event.keyCode) {
                    $(this).next().focus();
                }
                if (40 == event.keyCode) {
                    $(this).next().focus();
                }
                if(jQuery.inArray(event.keyCode, [112,113,119,114,115,116,117,118,120,121]) != -1){
                    return false;
                }

            }
        });
    });
</script>

