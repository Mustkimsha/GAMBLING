    function  ticketRangeConfirmOk(is_confirm) {

        var data = $('#anysearch-input').val();
        var dataValidation = data.split(".");

        if(data == ''){
            alertData('Enter receipt number');
            return false;
        }
        var agentType = "agent_2";
        if(agentType == 'agent_3' || agentType == 'agent_2'){
            //if agent is  agbnet_3 then only one ticket number field is required.
            if(dataValidation[0] == undefined)
            {
                alertData('Invalid details.');
                return false;
            }
        }else{
            //if agent is  agent_1 then all three details required
            if(dataValidation[0] == undefined || dataValidation[1] == undefined || dataValidation[2] == undefined)
            {
                alertData('Invalid details.');
                return false;
            }
        }

        $.ajax({
            type: 'POST',
            url: "~/agent/program/ticket-scaning",
            data: {data:data,agentType:agentType,'is_confirm':true},
            headers: {
                "X-CSRF-TOKEN": "lMjBiqe5e6bCw8hLWHOEWpuziz9QLCaae3BF03Va",
            },
            beforeSend: function () {
            },
            success: function (result) {
                $("#ticket-range-confirm").removeClass("visible");
                $(".popup-overlay").removeAttr("style");

                if(result.message == 'empty'){
                    alertData('Receipt not found.');
                    return false;
                }
                if(result.message == 'no_winner'){
                    $("#anysearch-input").val('');
                    alertData('Sorry...There is no winning yantra');
                    return false;
                }
                if(result.message == 'some_ticket_scanned'){
                    $("#anysearch-input").val('');
                    alertData('Some ticket number already scanned.');
                    return false;
                }
                if(result.message == 'draw_pending'){
                    $("#anysearch-input").val('');
                    alertData('Draw pending');
                    return false;
                }
                if(agentType == 'agent_2'){
                    if(result.message == 'invalid_details'){
                        alertData('Invalid details.');
                        return false;
                    }
                }
                if(result.message == 'cancelled'){
                    $("#anysearch-input").val('');
                    alertData('This receipt number was canceled.');
                    return false;
                }
                if(result.message == 'ticket_no_range_available'){
                    ticketRangeConfirmation('For this Yantra, you have a ticket no range available.');
                    return false;
                }

                if(result.success == false){
                    //already ticket scan message
                    $("#anysearch-input").val('');
                    alertData(result.message);
                    return false;
                }

                $("#anysearch-input").val('');
                getBalance();
                $('#winPrint').html(result);
                var msg = $('.message').text();
                alertData(msg);

                //get print dialog box without open tab/pop window
                var frame1 = document.createElement('iframe');
                frame1.name = "frame1";
                frame1.style.position = "absolute";
                frame1.style.top = "-1000000px";
                document.body.appendChild(frame1);
                var frameDoc = frame1.contentWindow ? frame1.contentWindow : frame1.contentDocument.document ?
                frame1.contentDocument.document : frame1.contentDocument;
                frameDoc.document.open();
                frameDoc.document.write(result);
                frameDoc.document.close();
                setTimeout(function () {
                    window.frames["frame1"].focus();
                    window.frames["frame1"].print();
                    document.body.removeChild(frame1);
                }, 500);

                $("#Shree").focus();
                return false;
            },
            error: function (response) {
                console.log('something went wrong');
            },
        });
    }
