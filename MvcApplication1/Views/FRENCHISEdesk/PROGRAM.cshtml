                                        // Access the "getHours" method on the Date object with the dot accessor.
                                            hours = time.getHours(),

                                        // Access the "getMinutes" method with the dot accessor.
                                            minutes = time.getMinutes(),


                                            seconds = time.getSeconds();

                                        

                                        var ampm = hours >= 12 ? 'PM' : 'AM';
                                        hours = hours % 12;
                                        hours = hours ? hours : 12; // the hour '0' should be '12'
                                        var temptime = harold(hours) + ":" + harold(minutes) + ":" + harold(seconds) + " " + ampm;
                                        document.getElementById("clock").value = harold(hours) + ":" + harold(minutes) + ":" + harold(seconds) + " " + ampm;

                                        var dt = time;
                                        var mm = dt.getMonth() + 1;
                                        mm=harold(mm);
                                        var dd = dt.getDate();
                                        var yyyy = dt.getFullYear();
                                        var format = harold(dd) + '-' + mm + '-' + yyyy;

                                        
                                        yesterday.setDate(yesterday.getDate()-1);
                                        var Datetxt = time.toLocaleDateString();
                                        var YesDatetxt = yesterday.toLocaleDateString();
                                        document.getElementById("Datetxtli").value=Datetxt;
                                        document.getElementById("datetoday").innerHTML=format;
                                        document.getElementById("timetoday").innerHTML=temptime;
                                        document.getElementById("datetoday2").innerHTML=format;
                                        document.getElementById("timetoday2").innerHTML=temptime;
                                        
                                        function harold(standIn) {
                                            if (standIn < 10) {
                                                standIn = '0' + standIn
                                            }
                                            return standIn;
                                        }
                                        $.post("/FRENCHISEdesk/programtime", { todaytimenow: temptime}, function (json) {
                                            document.getElementById("gsendtime").innerHTML=json.GSENDTIME;
                                            document.getElementById("gstimer").innerHTML=json.GSTIME;
                                            document.getElementById("fretotalbal").innerHTML=json.GSID;

                                            document.getElementById("gsendtime2").innerHTML=json.GSENDTIME;
                                            document.getElementById("gstimer2").innerHTML=json.GSTIME;

                                            var winyt = json.GSENDTIME;

                                            var time = json.GSTIME;
                                            var hours = Number(time.match(/^(\d+)/)[1]);
                                            var minutes = Number(time.match(/:(\d+)/)[1]);
                                            var AMPM = time.match(/\s(.*)$/)[1];
                                            if(AMPM == "PM" && hours<12) hours = hours+12;
                                            if(AMPM == "AM" && hours==12) hours = hours-12;
                                            var sHours = hours.toString();
                                            var sMinutes = minutes.toString();
                                            if(hours<10) sHours = "0" + sHours;
                                            if(minutes<10) sMinutes = "0" + sMinutes;
                                            time2.setHours(sHours);
                                            time2.setMinutes(sMinutes);
                                            time2.setSeconds(00);
                                            t1 = time2.getTime();
                                            t2 = time3.getTime();
                                            timeleft = t1 - t2;
                                            
                                            var minutes = Math.floor((timeleft % (1000 * 60 * 60)) / (1000 * 60));
                                            var seconds = Math.floor((timeleft % (1000 * 60)) / 1000);
                                            document.getElementById("gstimer").innerHTML=harold(minutes) + ":" + harold(seconds);
                                            document.getElementById("gstimer2").innerHTML=harold(minutes) + ":" + harold(seconds);
                                            
                                            var mstr = harold(minutes);
                                            var sstr = harold(seconds);

