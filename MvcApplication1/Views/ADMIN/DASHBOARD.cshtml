<script>
    function mygeneratepool() {
        var timestamps;
        var time;
        
        $.get("https://vip.timezonedb.com/v2.1/get-time-zone?key=BSWYV6J8NXWD&format=json&by=zone&zone=Asia/Kolkata")
            .done(function (json) {
                console.log(json);
                timestamps = json.formatted;
                timeampm = new Date(timestamps);

                time = new Date(timestamps);

                var h = time.getHours();
                var m = time.getMinutes();
                if (h < 8)
                {
                    var Datetxt = time.toLocaleDateString();

                    $.post("/ADMIN/creategamepool", { todaydate: Datetxt }, function (json) {
                        document.getElementById("datatry").innerHTML = json;
                        alert(json);
                    }
                    )
                }
                else {
                    alert("please enter after date change");
                }
            });
    }
</script>